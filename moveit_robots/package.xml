  <run_depend>atlas_moveit_config/</run_depend>
  <run_depend>atlas_v3_moveit_config/</run_depend>
  <run_depend>baxter/baxter_ikfast_left_arm_plugin</run_depend>
  <run_depend>baxter/baxter_ikfast_right_arm_plugin</run_depend>
  <run_depend>baxter/baxter_moveit_config/</run_depend>
  <run_depend>clam_moveit_config/</run_depend>

<?xml version="1.0"?>
<package>
  <name>moveit_robots</name>
  <version>0.0.0</version>
  <description>moveit_robots meta-package contains multiple robots moveit configuration packages.</description>

  <maintainer email="robot.moveit@gmail.com ">Sachin Chitta</maintainer>
  <author email="">Isaac I.Y. Saito</author>
  <license>BSD</license>
  <url type="website">http://wiki.ros.org/moveit_robots</url>

  <buildtool_depend>catkin</buildtool_depend>
  <build_depend>atlas_moveit_config/</build_depend>
  <build_depend>atlas_v3_moveit_config/</build_depend>
  <build_depend>baxter/baxter_ikfast_left_arm_plugin</build_depend>
  <build_depend>baxter/baxter_ikfast_right_arm_plugin</build_depend>
  <build_depend>baxter/baxter_moveit_config/</build_depend>
  <build_depend>clam_moveit_config/</build_depend>
